<div class="container center-align entireLogin">
    <div class="loginHead">
        <h1>Login</h1>
        <p>Please enter your information to log in.</p>
    </div>


    <div class="row">
        <form action="/login" method="POST" class="col s12 center-align">

            <div class="row">
                <div class="input-field col offset-s3 s6 offset-m3 m6 offset-l3 l6">
                    <input id="user" type="text" class="validate" name="userName">
                    <label for="user">Username:</label>
                </div>
            </div>

            <div class="row">
                <div class="input-field col offset-s3 s6 offset-m3 m6 offset-l3 l6">
                    <input id="password" type="password" class="validate" name="password">
                    <label for="password">Password:</label>
                </div>
            </div>

            <button type="submit" class="waves-effect waves-light btn">Login</button>
        </form>
    </div>
</div>


